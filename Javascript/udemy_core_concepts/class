// class is another type of writing object/prototype
//for class hoisting is not implemented
class Vehicle {
  constructor(numWheels, price) {
    this.numWheels = numWheels;
    this.price = price;
  }

  getPrice() {
    return this.price;
  }
}

const vehicle1 = new Vehicle(2, 50000);
const vehicle2 = new Vehicle(4, 500000);

